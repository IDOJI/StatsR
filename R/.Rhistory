labs(title = paste0("Correlation of `", y, "` with Other Variables"),
x = "Variables",
y = "Correlation Coefficient") +
theme_minimal() +
scale_fill_gradientn(colours = colors) +
scale_y_continuous(limits = c(min(cor.df$Correlation) - 0.1, max(cor.df$Correlation) + 0.1)) +
theme(
